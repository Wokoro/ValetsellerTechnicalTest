{
  "name": "ValetsellerTechnicalTest",
  "version": "1.0.0",
  "description": "Valetseller Technical Test",
  "main": "index.js",
  "engines": {
    "node": "14.15.4",
    "npm": "8.0.0"
  },
  "scripts": {
    "start:dev": "nodemon ./src/index.js --exec ./node_modules/.bin/babel-node --ext js,json,yaml",
    "start": "nodemon ./dist/index.js",
    "test:dev": "nyc mocha ./src/tests/index.js --exit",
    "test": "nyc mocha ./src/tests/index.js --exit",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all",
    "build": "babel ./src --out-dir ./dist --compact --ignore test --minified --no-comments --copy-files"
  },
  "license": "LICENSE",
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/node": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.13.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/register": "^7.13.0",
    "@babel/runtime": "^7.13.4",
    "@types/express-fileupload": "^1.1.6",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.3.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.4",
    "sinon-chai": "3.5.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "5.2.6",
    "express-validator": "^6.10.0",
    "helmet": "^4.4.1",
    "hpp": "0.2.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "sequelize": "^6.4.0",
    "sequelize-cli": "^5.0.0",
    "swagger-jsdoc": "^3.7.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  }
}
